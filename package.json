{
  "name": "star-wars-pong",
  "version": "1.0.0",
  "description": "Star Wars themed Pong game with AI opponent and mobile touch controls",
  "main": "starwars-pong.html",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:report": "playwright show-report",
    "start": "http-server . -p 8080",
    "dev": "http-server . -p 8080 -o",
    "build": "echo 'No build step needed for static HTML game'",
    "deploy": "gh-pages -d .",
    "lint": "echo 'No linting configured for static HTML game'",
    "format": "echo 'No formatting configured for static HTML game'"
  },
  "keywords": [
    "game",
    "pong",
    "star-wars",
    "html5",
    "canvas",
    "mobile",
    "touch-controls",
    "ai"
  ],
  "author": "welltodobusinessman",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "http-server": "^14.1.1",
    "gh-pages": "^6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/welltodobusinessman/Star-wars-pong.git"
  },
  "bugs": {
    "url": "https://github.com/welltodobusinessman/Star-wars-pong/issues"
  },
  "homepage": "https://welltodobusinessman.github.io/Star-wars-pong/"
}